{
  "name": "postcss-shadowCSS",
  "version": "0.1.1",
  "description": "PostCSS plugin to provide element-like CSS references for components, their modifiable subcomponents, and their custstomizable states",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "shadow-DOM"
  ],
  "author": "Nick Gard <ngard@walmartlabs.com>",
  "license": "MIT",
  "repository": "NickGard/postcss-shadowCSS",
  "bugs": {
    "url": "https://github.com/NickGard/postcss-shadowCSS/issues"
  },
  "homepage": "https://github.com/NickGard/postcss-shadowCSS",
  "dependencies": {
    "postcss": "^5.1.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "eslint": "^3.1.1",
    "eslint-config-postcss": "^2.0.2"
  },
  "scripts": {
    "test": "ava && eslint *.js"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss/es5",
    "rules": {
      "no-extra-parens": "off",
      "indent": ["error", 2],
      "max-len": "off"
    }
  }
}
